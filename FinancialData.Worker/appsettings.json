{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.Hosting.Lifetime": "Information",
        "Microsoft.EntityFrameworkCore.Database": "Warning",
        "System.Net.Http.HttpClient.ITimeSeriesClient.LogicalHandler": "Warning"
      }
    },
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.File",
      "Serilog.Sinks.Debug"
    ],
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "Debug"
      },
      {
        "Name": "File",
        "Args": {
          "path": "./bin/logs/log-.txt",
          "rollingInterval": "Day",
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff} [{Level:u3}] {SourceContext} {Message:lj}{NewLine}{Exception}"
        }
      }
    ],
    "Enrich": [
      "FromLogContext"
    ]
  },
  "TwelveDataTokenBucketLimiterOptions": {
    "TokenLimit": 8,
    "TokensPerPeriod": 8,
    "ReplenishmentPeriodMinutes": 1,
    "QueueLimit": 40,
    "QueueProcessingOrder": "OldestFirst",
    "AutoReplenishment": true
  },
  "TimeSeriesClientOptions": {
    "BaseUrl": "https://api.twelvedata.com/",
    "TimeoutMinutes": 5
  },
  "DelayMinutes": 1440,
  "ArgumentsPath": "Arguments"
}
