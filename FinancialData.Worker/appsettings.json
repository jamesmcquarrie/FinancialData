{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information",
      "Microsoft.EntityFrameworkCore.Database": "Warning",
      "System.Net.Http.HttpClient.ITimeSeriesClient.LogicalHandler": "Warning"
    }
  },
  "ApplicationTokenBucketLimiterOptions": {
    "TokenLimit": 8,
    "TokensPerPeriod": 8,
    "ReplenishmentPeriodMinutes": 1,
    "QueueLimit": 40,
    "QueueProcessingOrder": "OldestFirst",
    "AutoReplenishment": true
  },
  "TimeSeriesClientOptions": {
    "BaseUrl": "https://api.twelvedata.com/",
    "TimeoutMinutes": 5
  },
  "DelayMinutes": 1440,
  "ArgumentsPath": "Arguments"
}
